    "@types/pg": "^8.15.6",
    "@types/ws": "^8.5.13",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.31.7",
    "esbuild": "^0.25.0",
    "http-proxy-middleware": "^2.0.6",
    "nodemon": "^3.1.11",
    "tsx": "^4.21.0",
    "typescript": "5.6.3"
